{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"jsx": "react-jsx",
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"baseUrl": "./",
		"paths": {
			"@atoms/*": ["src/components/atoms/*"],
			"@molecules/*": ["src/components/molecules/*"],
			"@organisms/*": ["src/components/organisms/*"]
		}
	}
}
